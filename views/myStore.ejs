    <% items.forEach(function(image) { %>
        <div class="Facilitycontainer">
            <div class="Cardcontainer">
                <div class="Fcard">
                    <div class="Fcontent">
                        <div class="iconborder">
                            <img class="iconImage" src="data:image/<%=image.img.contentType%>;base64,<%=image.img.data.toString('base64')%>" alt="">
                        </div>
                        <br>
                        <h3><%= image.itemName %></h3>
                    </div>
                    <p class="description">Price: 500/- 
                       <%= image. desc %></p>
                </div> 
                <%});%>           
                
            </div>
        </div>  



